# rpaf module configuration file, 
# generated by Chef for <%= node[:fqdn] %>
<IfModule rpaf_module>
# use module identifier (rpaf_module) instead of filename (mod_rpaf-2.0.c) to workaround
# https://bugs.launchpad.net/ubuntu/+source/libapache2-mod-rpaf/+bug/1002571
RPAFenable On
RPAFsethostname On
RPAFproxy_ips 127.0.0.1
</IfModule>
